#!/usr/bin/env ruby

require 'thor'
require_relative './../lib/group_mixer'

class GroupMixer::Command < Thor
  default_command :execute

  desc '', 'execute command'
  def execute
    people = []
    past_set = [[]]
    group_size = 4
    is_separate_reminders = false

    p GroupMixer.by_group_size(people, past_set, group_size)
  end
end

GroupMixer::Command.start(ARGV)
